{
  "name": "VLLM Dev",
  "dockerFile": "../Dockerfile",
  "runArgs": ["--gpus", "all"],
  "mounts": [
      "source=${localWorkspaceFolder},target=/workspace,type=bind,consistency=cached"
  ],
  "workspaceFolder": "/workspace",
  "extensions": [
      "ms-python.python",
      "ms-python.pylint"
  ],
  "forwardPorts": [8000],
  "postCreateCommand": "pip install -e ."
}